[MASTER]
ignore=tests

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-instance-attributes,
    no-member,
    no-self-use,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    too-many-public-methods,
    duplicate-code